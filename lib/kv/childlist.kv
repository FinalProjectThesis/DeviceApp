#:import Factory kivy.factory.Factory
<ChildListScreen>:
    name: 'childlist'
    BoxLayout:
        orientation: 'vertical'
        Label:
            id: test_label
            text: 'Chid List'
            font_size: dp(50)
        ScrollView:
            BoxLayout:
                id: scroll_child
                name: 'scroll_child'
                size_hint: None, 1
                width: self.minimum_width
                padding: dp(20)
                spacing: dp(15)

        AnchorLayout:
            anchor_x: 'right'
            anchor_y: 'bottom'
            padding: dp(15)
            Button:
                size_hint: None, None
                height: dp(40)
                width: dp(100)
                text: 'Logout'
                on_press: Factory.LogoutPop().open()
        # Button:
        #     text: 'test_btn'
        #     on_press: root.removeBox()

<LogoutPop@Popup>:
    id: logout_pop
    auto_dismiss: False
    title: 'Are you sure you want to Logout?'
    size_hint: None, None
    size: dp(300), dp(100)
    BoxLayout:
        Button:
            text: 'Logout'
            on_press: app.root.token = ''
            on_release: app.root.current = 'login'; root.dismiss()
        Button:
            text: 'No'
            on_release: root.dismiss()