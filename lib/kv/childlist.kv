#:import Factory kivy.factory.Factory
<ChildListScreen>:
    name: 'childlist'
    BoxLayout:
        orientation: 'vertical'
        Label:
            size_hint: 1, .5
            id: test_label
            text: "Who's Learning?"
            font_size: dp(50)
        ScrollView:
            size_hint: 1, 1
            BoxLayout:
                id: scroll_button
                name: 'scroll_child'
                size_hint: None, 1
                width: self.minimum_width
                padding: dp(15)
                spacing: dp(20)
        AnchorLayout:
            size_hint: 1, .3
            anchor_x: 'right'
            anchor_y: 'bottom'
            padding: dp(15)
            MDRaisedButton:
                spacing: dp(20)
                text: 'Logout'
                md_bg_color: .8, 0, 0, 1
                on_press: root.on_logout()
