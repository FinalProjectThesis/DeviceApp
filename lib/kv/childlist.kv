#:import Factory kivy.factory.Factory
<ChildListScreen>:
    name: 'childlist'
    Image:
        source: 'assets\images\studentlist_background.jpg'
        allow_stretch: True
        keep_ratio: False
        size: self.texture_size
        repeat: True
    BoxLayout:
        id: main_layout
        orientation: 'vertical'
        Image:
            source: "assets\images\Whos_Learning.png"
            size_hint: 1, .5
        # MDLabel:
        #     size_hint: 1, .5
        #     id: title_label
        #     text: "Who's Learning?"
        #     font_size: dp(50)
        #     outline_color: 1, 0, 0, 1
        #     halign: 'center'
        ScrollView:
            id: scroll_view
            size_hint_y: 1
            BoxLayout:
                id: scroll_button
                size_hint: None, 1
                width: self.minimum_width
                padding: dp(15)
                spacing: dp(20)
        AnchorLayout:
            size_hint: 1, .3
            anchor_x: 'right'
            anchor_y: 'bottom'
            padding: dp(15)
            MDRaisedButton:
                spacing: dp(20)
                text: 'Logout'
                md_bg_color: .8, 0, 0, 1
                on_release: root.on_logout()
